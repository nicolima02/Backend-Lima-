{
  "name": "clase_8",
  "version": "1.0.0",
  "description": "Para realizar el test de artillery en modo cluster, iniciar el servidor con node src/services/server.js --CLUSTER y despues en otra terminal ejecutar el comando npm run artillery.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/index.js",
    "start:mongo": "node src/index.js MONGO",
    "start:file": "node src/index.js FILE",
    "dev": "nodemon src/index.js",
    "artillery:fork": "artillery quick --count 50 -n 20 http://localhost:8080/info > resultado_artilleryFORK.txt",
    "artillery:cluster": "artillery quick --count 50 -n 20 http://localhost:8080/info > resultado_artilleryCLUSTER.txt",
    "inspect": "node --inspect src/index.js",
    "profiling": "node --prof-process isolate-bloq.log > isolateFinal.txt",
    "autocannon": "autocannon -c 100 -d 20 http://localhost:8080/info",
    "0x": "0x src/services/server.js",
    "start:cluster": "node src/index.js --CLUSTER",
    "prof": "node --prof src/services/server.js",
    "mocha": "mocha src/test/test.js",
    "adonis:help": "adonis --help",
    "jest": "jest"
  },
